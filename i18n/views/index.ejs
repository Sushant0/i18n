<html>
  <head>
    <!-- Material Design Lite -->
    <script src="https://code.getmdl.io/1.1.3/material.min.js"></script>
    <link rel="stylesheet" href="https://code.getmdl.io/1.1.3/material.indigo-pink.min.css">
    <!-- Material Design icon font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

    <script src="https://code.getmdl.io/1.1.3/material.min.js"></script>
    <link rel="stylesheet" href="https://code.getmdl.io/1.1.3/material.indigo-pink.min.css">
    <!-- Material Design icon font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

<style>
.header {
    width: 100%;
    position: fixed;
    top: 0;
    bottom: auto;
}
.outer {
    position: relative;
    height: 100px;
}
.outer .header {
    position: absolute;
    bottom: 0;
    z-index: 2;
    top: auto;
}
/*.content {
    height: 1500px;
    margin-top: 100px;
}*/
#wrapper {
  width:600px;
  clear:both;
  text-align: center;
  left: 25em;
  position: fixed;

  z-index: 2;
  border: 1px solid #ccc;
  background-color: #f3f3f3;"

}
#first {
  width:200px;
  float:left;
}
#second {
  width:200px;
  float:left;
}
#third {
  width:200px;
  float:left;
}
  </style>

  </head>
  <body>
    <div id="wrapper">
      <div id="first">
        <form action="/generateIos" method="get" style="padding-top: 10px;">
        <button type="submit" value="IOS"> IOS </button></form>
      </div>
      <div id="second">
        <form action="/generateAndroid" method="get" style="padding-top: 10px;">
        <button type="submit" value="Android"> Andriod </button></form>
      </div>
      <div id="third" style="padding-top: 10px;">
        <button type="button" value="Submit" onClick = "openAddDialog()" > Add </button>
      </div>
    </div>

        <table id="holder" class="mdl-data-table mdl-js-data-table" style="display:none;
        position: fixed;
        width:45em;
        z-index: 2;
        left: 25em;
        border: 1px solid #ccc;
        background-color: #f3f3f3;" align="center">
        	  <thead>
              <tr >
                  <th class="mdl-data-table__cell--non-numeric">Key</th>
                  <th>Value</th>
                  <th>IOS</th>
                  <th>Andriod</th>
                  <th>Verified</th>
                </tr>
              </thead>
              <tbody>
                <form action="/insert" method="post">
                <tr>
                  <td >
                 	  <input type="text" name="key" value="">
                  </td>
                  <td>
                    <input type="text" name="value" value="">
                  </td>
                  <td>
                    <input type="checkbox" name="isIOS" value="on">
                  </td>

                  <td>
                    <input type="checkbox" name="isAndriod" value="on">
                  </td>

                    <td>
                      <input type="checkbox" name="isVerified" value="on">
                    </td>
                </tr>
                <tr>
                  <td>
                    <button type="submit" value="Submit" onClick = "closeAddDialog()"> Add </button>
                  </td>
                  <td>
                    <button type="submit" value="Submit" onClick = "closeAddDialog()"> Close </button>
                  </td>
                  <td>
                  </td>
                  <td>
                  </td>
                  <td>
                  </td>
                </tr>
              </form>
              </tbody>
          </table>
    </div>
</div>

	<table class="mdl-data-table mdl-js-data-table" style="position:absolute;
    top: 10%;
    left: 25%;
    width:30em;
    height:18em;
    border: 1px solid #ccc;
    background-color: #f3f3f3;" align="center" >
	  <thead>
	    <tr>
	      <th class="mdl-data-table__cell--non-numeric">Key</th>
	      <th>Value</th>
	      <th>IOS</th>
	      <th>Andriod</th>
	      <th>Verified</th>
	      <th></th>
	      <th></th>
	    </tr>
	  </thead>
	  <tbody>
		  <%var test = "hello world"; list.forEach(function(val){ %>
		  		<tr>
		  		<form action="/post" method="post">
			  		<input type="hidden" name="key" value="<%= val.getKey(); %>" />
			  		<td class="mdl-data-table__cell--non-numeric"><%= val.getKey(); %></td>
				    <td><input type="label" name="value" value="<%= val.getValue() %>"></td>
				    <td><input type="checkbox" name="isIOS"
				    	<% if(val.getIsIOS()){ %>
					    	<%= "checked" %>
				    	<% } %>
			    	></td>
				    <td><input type="checkbox" name="isAndriod"
					    <% if(val.getIsAndriod()){ %>
					    	<%= "checked" %>
				    	<% } %>
			    	></td>
				    <td><input type="checkbox" name="isVerified"
					    <% if(val.getIsVerified()){ %>
					    	<%= "checked" %>
				    	<% } %>
			    	></td>
				    <td><button type="submit" name="Update"> Update </button></td>
				    <td><button type="submit" name="Remove"> Remove </button></td>
			    </form>
				</tr>
		   <% }); %>
	  </tbody>
	</table>

<script type="text/javascript">
function openAddDialog() {
   document.getElementById('holder').style.display = "block";
   document.getElementById('wrapper').style.display = "none";

}
function closeAddDialog() {
    document.getElementById('holder').style.display = "none";
    document.getElementById('wrapper').style.display = "block";
}
</script>
  </body>
</html>
