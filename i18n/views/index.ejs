<html>
  <head>
    <!-- Material Design Lite -->
    <script src="https://code.getmdl.io/1.1.3/material.min.js"></script>
    <link rel="stylesheet" href="https://code.getmdl.io/1.1.3/material.indigo-pink.min.css">
    <!-- Material Design icon font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
  </head>
  <body>
    <div id="holder" style = " width:100%;display:none;">     
    <div id="popup" style = " background:#fff;padding: 28px 26px 33px 25px;">            
        <div is="content" style = " border-radius: 7px;background:#6b6a63;margin:30px auto 0;padding:6px;position:absolute;width:400px;top: 50%;left: 50%;margin-left: -400px;margin-top: -40px;"> 
                    <form action="/insert" method="post">
                    Key :  <input type="text" name="vehicle" value="Bike">
                    Value :<input type="text" name="vehicle" value="Bike">
                    Android :<input type="checkbox" name="vehicle" value="Bike">
                    Ios :<input type="checkbox" name="vehicle" value="Bike">
                    Verified :<input type="checkbox" name="vehicle" value="Bike">
                    <button type="submit" value="Submit"> Add </button>
                </form>
        </div>
    </div>
</div>
	<table class="mdl-data-table mdl-js-data-table" style="margin: auto;" >
	  <thead>
	    <tr>
	      <th class="mdl-data-table__cell--non-numeric">Key</th>
	      <th>Value</th>
	      <th>IOS</th>
	      <th>Andriod</th>
	      <th>Verified</th>
	      <th></th>
	      <th></th>
	    </tr>
	  </thead>
	  <tbody>
		  <% list.forEach(function(val){ %>
		  		<tr>
		  		<form action="/" method="post">
		  		<% console.log("hello world" + val.getValue() + val.getIsIOS()); %>
			  		<td class="mdl-data-table__cell--non-numeric"><%= val.getKey(); %></td>
				    <td><input type="text" name=<%= val.getKey(); %> value=<%= val.getValue(); %>></td>
				    <td><input type="checkbox" name="isIOS" 
				    	<% if(val.getIsIOS()){ %>
					    	<%= "checked" %>
				    	<% } %>
			    	></td>
				    <td><input type="checkbox" name="isAndriod"
					    <% if(val.getIsAndriod()){ %>
					    	<%= "checked" %>
				    	<% } %>
			    	></td>
				    <td><input type="checkbox" name="isVerified"
					    <% if(val.getIsVerified()){ %>
					    	<%= "checked" %>
				    	<% } %>
			    	></td>
				    <td><button type="submit" name="Update"> Update </button></td>
				    <td><button type="submit" name="Remove"> Remove </button></td>
			    </form>
				</tr>
		   <% }); %>
	  </tbody>
	</table>
	<div style="width: 500px; margin: 0 auto;">
		
			<button type="button" value="Submit"> IOS </button>
			<button type="button" value="Submit"> Andriod </button>
			<button type="button" value="Submit" onClick = "myFunction()"> Add </button>
	</div>
    <script type="text/javascript">
function myFunction() {
   document.getElementById('holder').style.display = "block";

}
function div_hide() {
    document.getElementById('abc').style.display = "none";
}
</script>
  </body>
</html>

