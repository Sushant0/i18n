<html>
  <head>
    <!-- Material Design Lite -->
    <script src="https://code.getmdl.io/1.1.3/material.min.js"></script>
    <link rel="stylesheet" href="https://code.getmdl.io/1.1.3/material.indigo-pink.min.css">
    <!-- Material Design icon font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
  </head>
  <body>
  	<div id="holder" style = " width:100%;display:none;">     
    <div id="popup" style = " background:#fff;padding: 28px 26px 33px 25px;">            
        	 <div is="content" style = " border-radius: 7px;background:#6b6a63;margin:30px auto 0;padding:6px;position:absolute;width:400px;height:200px;top: 50%;left: 50%;margin-left: -15em; margin-top: -8em;z-index:999;"> 
        	 	<div style = "margin-top: 3em">   
                    <form action="/insert" method="post">

                 	Key :  <input type="text" name="key" value="">
                    Value :<input type="text" name="value" value="">
                    <br>
                    <br>
                    <input type="checkbox" name="isIOS" value="on">Android 
                    <input type="checkbox" name="isAndriod" value="on">IOS
                    <input type="checkbox" name="isVerified" value="on">Verified
              
					<button type="submit" value="Submit" onClick = "closeAddDialog()"> Add </button>
                </form>
                <button type="submit" value="Submit" onClick = "closeAddDialog()"> Close </button>
            </div>
        </div>
    </div>
</div>

	<table class="mdl-data-table mdl-js-data-table" style="position:fixed;
    top: 50%;
    left: 50%;
    width:30em;
    height:18em;
    margin-top: -12em; 
    margin-left: -25em; 
    border: 1px solid #ccc;
    background-color: #f3f3f3;" align="center" >
	  <thead>
	    <tr>
	      <th class="mdl-data-table__cell--non-numeric">Key</th>
	      <th>Value</th>
	      <th>IOS</th>
	      <th>Andriod</th>
	      <th>Verified</th>
	      <th></th>
	      <th></th>
	    </tr>
	  </thead>
	  <tbody>
		  <% list.forEach(function(val){ %>
		  		<tr>
		  		<form action="/" method="post">
		  		<% console.log("hello world" + val.getValue() + val.getIsIOS()); %>
			  		<input type="hidden" name="key" value=<%= val.getKey(); %> />
			  		<td class="mdl-data-table__cell--non-numeric"><%= val.getKey(); %></td>
				    <td><input type="text" name="value" value=<%= val.getValue(); %>></td>
				    <td><input type="checkbox" name="isIOS" 
				    	<% if(val.getIsIOS()){ %>
					    	<%= "checked" %>
				    	<% } %>
			    	></td>
				    <td><input type="checkbox" name="isAndriod"
					    <% if(val.getIsAndriod()){ %>
					    	<%= "checked" %>
				    	<% } %>
			    	></td>
				    <td><input type="checkbox" name="isVerified"
					    <% if(val.getIsVerified()){ %>
					    	<%= "checked" %>
				    	<% } %>
			    	></td>
				    <td><button type="submit" name="Update"> Update </button></td>
				    <td><button type="submit" name="Remove"> Remove </button></td>
			    </form>
				</tr>
		   <% }); %>
	  </tbody>
	</table>

	<div style="position:fixed;
    top: 50%;
    left: 50%;
    width:5em;
    margin-top: -12em; 
    margin-left: -25em;">
		    <form action="/generateIos" method="get">
				<button type="submit" value="IOS"> IOS </button></form>
			                    
			<form action="/generateAndroid" method="get">
				<button type="submit" value="Android"> Andriod </button></form>


			<button type="button" value="Submit" onClick = "openAddDialog()"> Add </button>
	</div>

<script type="text/javascript">
function openAddDialog() {
   document.getElementById('holder').style.display = "block";

}
function closeAddDialog() {
    document.getElementById('holder').style.display = "none";
}
</script>
  </body>
</html>

